{
	"snippets.ultisnips.pythonPrompt": true,
	"languageserver": {
		"ruby-lsp": {
			"command": "ruby-lsp",
			"initializationOptions": {
				"experimentalFeaturesEnabled": false,
				"enabledFeatures": [
					"codeActions",
					"documentHighlights",
					"documentLink",
					"documentSymbols",
					"foldingRanges",
					"formatting",
					"hover",
					"inlayHint",
					"onTypeFormatting",
					"selectionRanges",
					"semanticHighlighting",
					"completion"
			],
			"formatter": "auto"
			},
			"filetypes": ["ruby"]
		}
	}
}
